{
    "manifest_version": 3,
    "name": "Bookmark Loader",
    "version": "1.0",
    "description": "Retrieve all bookmarks in Chrome",
    "author": "achraf",
    "permissions": ["bookmarks"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }